import abc
from typing import Any, List, Tuple

class MySQLConnectionAbstract(metaclass=abc.ABCMeta):
    converter: Any = ...
    def __init__(self, **kwargs: Any) -> None: ...
    @property
    def user(self) -> str: ...
    @property
    def server_host(self) -> str: ...
    @property
    def server_port(self) -> int: ...
    @property
    def unix_socket(self) -> str: ...
    @property
    @abc.abstractmethod
    def database(self) -> MySQLConnectionAbstract: ...
    @database.setter
    def database(self, value: Any) -> MySQLConnectionAbstract: ...
    @property
    def can_consume_results(self) -> bool: ...
    def config(self, **kwargs: Any) -> None: ...
    def get_server_version(self) -> Tuple[int, ...] | None: ...
    def get_server_info(self) -> str | None: ...
    @property
    @abc.abstractmethod
    def in_transaction(self) -> Any: ...
    def set_client_flags(self, flags: Any) -> List[int]: ...
    def isset_client_flag(self, flag: Any) -> bool: ...
    @property
    def time_zone(self) -> str: ...
    @time_zone.setter
    def time_zone(self, value: Any) -> None: ...
    @property
    def sql_mode(self) -> str: ...
    @sql_mode.setter
    def sql_mode(self, value: Any) -> None: ...
    @abc.abstractmethod
    def info_query(self, query: Any) -> MySQLCursorAbstract: ...
    def set_login(
        self, username: Any | None = ..., password: Any | None = ...
    ) -> None: ...
    def set_unicode(self, value: bool = ...) -> None: ...
    @property
    def autocommit(self) -> bool: ...
    @autocommit.setter
    def autocommit(self, value: Any) -> None: ...
    @property
    def get_warnings(self) -> bool: ...
    @get_warnings.setter
    def get_warnings(self, value: Any) -> None: ...
    @property
    def raise_on_warnings(self) -> bool: ...
    @raise_on_warnings.setter
    def raise_on_warnings(self, value: Any) -> None: ...
    @property
    def unread_result(self) -> bool: ...
    @unread_result.setter
    def unread_result(self, value: bool) -> bool: ...
    @property
    def charset(self) -> str: ...
    @property
    def python_charset(self) -> str: ...
    def set_charset_collation(
        self, charset: Any | None = ..., collation: Any | None = ...
    ) -> None: ...
    @property
    def collation(self) -> str: ...
    @abc.abstractmethod
    def disconnect(self) -> Any: ...
    close: Any = ...
    def connect(self, **kwargs: Any) -> MySQLConnectionAbstract: ...
    def reconnect(self, attempts: int = ..., delay: int = ...) -> None: ...
    @abc.abstractmethod
    def is_connected(self) -> Any: ...
    @abc.abstractmethod
    def ping(
        self, reconnect: bool = ..., attempts: int = ..., delay: int = ...
    ) -> Any: ...
    @abc.abstractmethod
    def commit(self) -> Any: ...
    @abc.abstractmethod
    def cursor(
        self,
        buffered: Any | None = ...,
        raw: Any | None = ...,
        prepared: Any | None = ...,
        cursor_class: Any | None = ...,
        dictionary: Any | None = ...,
        named_tuple: Any | None = ...,
    ) -> MySQLCursorAbstract: ...
    @abc.abstractmethod
    def rollback(self) -> Any: ...
    def start_transaction(
        self,
        consistent_snapshot: bool = ...,
        isolation_level: Any | None = ...,
        readonly: Any | None = ...,
    ) -> None: ...
    def reset_session(
        self,
        user_variables: Any | None = ...,
        session_variables: Any | None = ...,
    ) -> None: ...
    def set_converter_class(self, convclass: Any) -> None: ...
    @abc.abstractmethod
    def get_rows(
        self,
        count: Any | None = ...,
        binary: bool = ...,
        columns: Any | None = ...,
    ) -> Any: ...
    def cmd_init_db(self, database: Any) -> None: ...
    def cmd_query(
        self,
        query: Any,
        raw: bool = ...,
        buffered: bool = ...,
        raw_as_string: bool = ...,
    ) -> None: ...
    def cmd_query_iter(self, statements: Any) -> None: ...
    def cmd_refresh(self, options: Any) -> None: ...
    def cmd_quit(self) -> None: ...
    def cmd_shutdown(self, shutdown_type: Any | None = ...) -> None: ...
    def cmd_statistics(self) -> None: ...
    def cmd_process_info(self) -> None: ...
    def cmd_process_kill(self, mysql_pid: Any) -> None: ...
    def cmd_debug(self) -> None: ...
    def cmd_ping(self) -> None: ...
    def cmd_change_user(
        self,
        username: str = ...,
        password: str = ...,
        database: str = ...,
        charset: int = ...,
    ) -> None: ...
    def cmd_stmt_prepare(self, statement: Any) -> None: ...
    def cmd_stmt_execute(
        self,
        statement_id: Any,
        data: Any = ...,
        parameters: Any = ...,
        flags: int = ...,
    ) -> None: ...
    def cmd_stmt_close(self, statement_id: Any) -> None: ...
    def cmd_stmt_send_long_data(
        self, statement_id: Any, param_id: Any, data: Any
    ) -> None: ...
    def cmd_stmt_reset(self, statement_id: Any) -> None: ...
    def cmd_reset_connection(self) -> None: ...

class MySQLCursorAbstract(metaclass=abc.ABCMeta):
    arraysize: int = ...
    def __init__(self) -> None: ...
    @abc.abstractmethod
    def callproc(self, procname: Any, args: Any = ...) -> Any: ...
    @abc.abstractmethod
    def close(self) -> Any: ...
    @abc.abstractmethod
    def execute(self, operation: Any, params: Any = ..., multi: bool = ...) -> Any: ...
    @abc.abstractmethod
    def executemany(self, operation: Any, seqparams: Any) -> Any: ...
    @abc.abstractmethod
    def fetchone(self) -> Any: ...
    @abc.abstractmethod
    def fetchmany(self, size: int = ...) -> Any: ...
    @abc.abstractmethod
    def fetchall(self) -> Any: ...
    def nextset(self) -> None: ...
    def setinputsizes(self, sizes: Any) -> None: ...
    def setoutputsize(self, size: Any, column: Any | None = ...) -> None: ...
    def reset(self, free: bool = ...) -> None: ...
    @property
    @abc.abstractmethod
    def description(self) -> Any: ...
    @property
    @abc.abstractmethod
    def rowcount(self) -> Any: ...
    @property
    @abc.abstractmethod
    def lastrowid(self) -> Any: ...
    def fetchwarnings(self) -> List[Tuple[str | int, ...]] | None: ...
